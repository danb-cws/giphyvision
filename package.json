    "lint": "eslint src/js/",
    "deploy": "firebase deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run lint && npm run serve-prod",
    "release": "npm run lint && npm run build && npm run deploy"
