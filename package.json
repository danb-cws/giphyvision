    "start-dev": "SET NODE_ENV=development&& node ./src/server.js",
    "serve": "SET NODE_ENV=production&& node ./src/server.js",
    "lint": "eslint src/js/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build & npm run serve"
